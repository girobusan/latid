
{% extends "page.njk" %}
{% block header%}
<h1>{{view.file.meta.title | nbsp}}</h1>
<small>{{ util.prettyDate(view.file.meta.date) }}</small>
<div class="row mb-5" >
<div class="group1 col-sm-12 col-md-12">
{% if view.tagslist and view.tagslist.length>0 %}
{% for tag in view.tagslist %}
<a href="{{tag.uri}}"><span class="badge badge-primary">{{tag.file.meta.title}}</span></a>
{% endfor %}
{% endif %}
  </div>
  </div>
{% endblock %}
{% block content %}
<div id="content" style="width: 100%">{{content}}</div>
{% endblock %}
