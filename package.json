{
  "name": "latid",
  "version": "1.8.14b",
  "description": "Staic site generator with visual editor",
  "main": "loader.js",
  "repository": {
    "type": "git",
    "url": "github:girobusan/latid"
  },
  "scripts": {
    "build": "npx webpack --mode production",
    "serve": "cd site && node latidserv.js -b",
    "watch": "npx webpack",
    "build_site_css": "lessc src/styles/site.less site/src/styles/main.css",
    "watch_cli": "npx webpack --config cli.wp.config.js",
    "build_cli": "npx webpack --config cli.wp.config.js --mode production",
    "build_all": "rm -R dist/* ; npm run build ; npm run build_cli ; cp -R dropins/dist/* dist"
  },
  "author": "GBT",
  "license": "MIT",
  "devDependencies": {
    "aws-sdk": "^2.618.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^3.2.0",
    "emotion": "^10.0.27",
    "fs": "0.0.1-security",
    "gray-matter": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lessc": "^1.0.2",
    "marked": "^1.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-pre-gyp": "^0.14.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "browser-or-node": "^1.2.1",
    "copy-dir": "^1.2.0",
    "d3": "^5.12.0",
    "feed-generator": "^1.0.6",
    "jsdom": "^15.2.1",
    "markdown-it": "^12.0.1",
    "markdown-it-multimd-table": "^4.0.3",
    "minimist": "^1.2.5",
    "nunjucks": "^3.2.1",
    "recursive-readdir": "^2.2.2",
    "smalltalk": "^4.0.6",
    "unidecode": "^0.1.8",
    "uuid": "^3.3.3",
    "webpack-node-externals": "^1.7.2"
  }
}
